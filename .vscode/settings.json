{
    "iis.configDir": "",
    "files.associations": {
        "**/scripts/*.sql": "postgres",
        "*.sql": "postgres"
    },
    "sql.linter.enabled": false,
    "mssql.enableQueryHistoryCapture": false,
    "mssql.intelliSense.enableIntelliSense": false,
    "mssql.enableConnectionPooling": false,
    "[sql]": {
        "editor.formatOnSave": false
    },
    "python.linting.enabled": true,
    "editor.formatOnSave": true,
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "sqltools.connections": [
        {
            "ssh": "Disabled",
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "driver": "PostgreSQL",
            "name": "Finnovate Postgres",
            "database": "finnovate",
            "username": "admin"
        }
    ]
}